{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "npm",
			"script": "watch",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": {
				"owner": "tsup",
				"source": "tsup",
				"applyTo": "closedDocuments",
				"severity": "error",
				"pattern":[
					{
						"regexp": "^âœ˜\\s+\\[ERROR\\]\\s+(.+)$",
						"message": 1
					},
					{
						"regexp": "^\\s*$"
					},
					{
						"regexp": "^\\s+(.+):(\\d+):(\\d+):$",
						"file": 1,
						"line": 2,
						"column": 3
					}
				],
				"background": {
					"beginsPattern": "^CJS Build start$",
					"endsPattern": "^CJS .* Build success|^CJS Build failed"
				}
			},
			"isBackground": true,
			"label": "npm: watch",
			"detail": "tsup --watch",
			"presentation": {
				"reveal": "never",
				"group": "watchers"
			}
		}
	]
}
